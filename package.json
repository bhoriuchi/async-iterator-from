    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-json-equal": "^0.0.1",
    "chai-spies-next": "^0.9.3",
    "chai-subset": "^1.6.0",
    "coveralls": "^3.0.3",
    "emit-stream": "^0.1.2",
    "eslint": "^5.16.0",
